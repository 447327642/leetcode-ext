{
  "name": "LeetCode-Ext",
  "version": "0.1.0",
  "description": "LeetCode.com Extension",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_title": "LeetCode Extension",
    "default_popup": ""
  },
  "permissions": [
    "storage",
    "https://chrome-ext.luxiakun.com/leetcode-ext",
    "https://api.github.com/"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problemset/algorithms/"],
      "css": [],
      "js": ["leetcode-ext.js"]
    },
    {
      "matches": ["https://leetcode.com/problems/*"],
      "css": [],
      "js": ["jquery.js", "webtoolkit.base64.js", "to-markdown.js", "leetcode-ext-github.js"]
    }
  ],
  "manifest_version": 2
}